	<% @pics.each do |pic|    %>
		<div class="stolen-frosty span4 text-center">

			<h1 class="stolen-h1">
				Lesson: <%= pic.lesson %>
				<br/>
			</h1>

			<h2>
				<%= link_to pic_path(pic) do %>
					<%= image_tag pic.image %>
				<% end %>
				<br/>
				Emmotion: <%= pic.emotion %>
				<br/>
				<br/>
				Learned: <%= pic.learned %>
				<br/>
			</h2>
		</div>
	<% end %>
